{
  "name": "jsondiff",
  "description": "A library for producing of diff of Javascript objects",
  "keywords": [
    "json",
    "diff",
    "recursive"
  ],
  "version": "1.0.0",
  "homepage": "https://github.com/pixelbits-mk/jsondiff",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelbits-mk/jsondiff.git"
  },
  "main": "dist/cjs.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "npm run build && tape test/*.js && jsmd readme.md && npm run test:typescript",
    "test:typescript": "tsc --noEmit test/typescript.ts && ts-node test/typescript.ts",
    "size": "npm run build && uglifyjs --compress --mangle -- ./dist/umd.js | gzip -c | wc -c"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "rollup": "^2.40.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tape": "^5.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "uglify-js": "^3.12.8"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pixelbits-mk/jsondiff/issues"
  },
  "dependencies": {}
}
